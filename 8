weekdays = {
    1: "Monday",
    2: "Tuesday",
    3: "Wednesday",
    4: "Thursday",
    5: "Friday",
    6: "Saturday",
    7: "Sunday"
}

def print_weekday(number):
    try:
        if 1 <= number <= 7:
            print(weekdays[number])
        else:
            raise ValueError("Invalid weekday number")
    except ValueError as e:
        print(e)
