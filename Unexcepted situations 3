import random

a = [12, 34, 54]
b = []
c = []

for i in range(0, 3):
    b.append(random.randrange(0, 5))

for i in range(len(a)):
    try:
        c.append(a[i] / b[i])
    except ZeroDivisionError:
        c.append('N/A')
        print("Cannot divide by zero.")
    except IndexError:
        c.append('N/A')
        print("Index out of range.")

print(a, b)
print(c)
